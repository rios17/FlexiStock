<h1>Editing despacho</h1>


<% form_for :despacho, @despacho,
:url => articulos_cliente_despacho_path(@articulos_cliente, @despacho),
:html => {:method => :put} do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :cantidad %><br />
    <%= f.text_field :cantidad %>
  </p>
  <p>
    <%= f.label :fecha_despacho %><br />
    <%= f.datetime_select :fecha_despacho %>
  </p>
  <p>
    <%= f.label :cliente_id %><br />
    <%= f.text_field :cliente_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', articulos_cliente_despachos_path %>