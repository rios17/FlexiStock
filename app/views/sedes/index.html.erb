<h1>Listing sedes</h1>

<table>
  <tr>
    <th>Nombre</th>
  </tr>

<% @sedes.each do |sede| %>
  <tr>
    <td><%=h sede.nombre %></td>
    <td><%= link_to 'Show', sede %></td>
    <td><%= link_to 'Edit', edit_sede_path(sede) %></td>
    <td><%= link_to 'Destroy', sede, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New sede', new_sede_path %>